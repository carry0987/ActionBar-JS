{
  "name": "@carry0987/action-bar",
  "version": "2.1.1",
  "description": "A library for create flat-styled float ActionBar, with selection count, and more",
  "type": "module",
  "main": "dist/actionBar.min.js",
  "module": "dist/actionBar.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "umd": "./dist/actionBar.min.js",
      "import": "./dist/actionBar.esm.js",
      "types": "./dist/index.d.ts"
    },
    "./theme/actionBar.min.css": "./dist/theme/actionBar.min.css"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.{ts,scss}\" \"test/**/*.{ts,tsx,scss}\"",
    "start": "rollup --config rollup.config.ts --configPlugin typescript",
    "build": "rollup --config rollup.config.ts --configPlugin typescript --environment BUILD:production",
    "test": "vitest run"
  },
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carry0987/ActionBar-JS.git"
  },
  "keywords": [
    "actionbar",
    "action",
    "operation",
    "bar"
  ],
  "author": "carry0987",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/carry0987/ActionBar-JS/issues"
  },
  "homepage": "https://github.com/carry0987/ActionBar-JS#readme",
  "devDependencies": {
    "@carry0987/utils": "^3.8.2",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "happy-dom": "^15.7.4",
    "prettier": "^3.3.3",
    "rollup": "^4.24.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-tsconfig-paths": "^1.5.2",
    "sass": "^1.80.3",
    "tslib": "^2.8.0",
    "vitest": "^2.1.3"
  }
}
